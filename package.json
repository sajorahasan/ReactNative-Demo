{
  "name": "ReactNativeDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 6'",
    "iphoneX": "react-native run-ios --simulator='iPhone X'",
