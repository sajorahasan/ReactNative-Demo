    "react-native-keyboard-aware-scroll-view": "0.8.0",
    "react-native-local-mongodb": "2.2.9",
    "react-native-localization": "2.1.2",
    "react-native-router-flux": "4.0.6",
    "react-native-simple-encryption": "1.2.1",
